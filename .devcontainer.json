{
	"name": "georga",
	"dockerComposeFile": [ "compose.yaml" ],
	"service": "server",
	"workspaceFolder": "/code",
	"shutdownAction": "none",
	"extensions": [
		"ms-python.python",
		"visualstudioexptteam.vscodeintellicode",
		"vscode.flake8",
		"ms-python.vscode-pylance"
	],
	"settings": {
		"editor.rulers": [ 119 ]
	}
}
